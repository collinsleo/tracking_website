
<%- include('../layout/admin_header.ejs') %>

<!-- Right side column. Contains the navbar and content of the page -->
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <h1>
      Parcel Registration
    </h1>
    <ol class="breadcrumb">
      <li><a href="/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
      <li><a href="/parcel">view Percel</a></li>
      <li class="active">New Parcel page</li>
    </ol>
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="row d-flex justify-content-center ">
      <div class="col-md-8">

        <div class="box box-danger">
          <div class="box-header">
            <h3 class="box-title">New Parcel</h3>
          </div>

            <!-- alert -->
            <% if(typeof error !== "undefined"){ %>
            <div class="alert alert-danger alert-dismissible show" role="alert">
              <strong>Error!</strong>  <%= error %>              
          </div>
          <% } %>
  
            <!-- end alert -->

          <form action="/newparcel" method="post">
            <div class="box-body">
              
              <div class="row">

                <div class="col-md-6">
                  <div class="form-group">
                    <label>Sender Name</label>
                    <input type="text" class="form-control" name="sName" placeholder="Enter Sender Name..."/>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="form-group">
                    <label>Sender phone:</label>
                    <input type="text" class="form-control" name="sPhone" placeholder="Enter Sender Phone Number..."/>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="form-group">
                    <label>Reciever Name</label>
                    <input type="text" class="form-control" name="rName" placeholder="Enter Reciever Name..."/>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="form-group">
                    <label>Receiver phone:</label>
                    <input type="text" class="form-control" name="rPhone" placeholder="Enter Reciever Phone Number..."/>
                  </div>
                </div>

                <div class="col-md-12">
                  <div class="form-group">
                    <label>receiver email:</label>
                    <input type="text" class="form-control" name="rEmail" placeholder="Enter Reciever Email..."/>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="form-group">
                    <label>pickup location</label>
                    <input type="text" class="form-control" name="pickup" placeholder="Enter pickup location..."/>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="form-group">
                    <label>parcel Destination:</label>
                    <input type="text" class="form-control" name="destination" placeholder="Enter parcel Destination..."/>
                  </div>
                </div>

                <div class="col-md-12">
                  <div class="form-group">
                    <label>weight:</label>
                    <input type="text" class="form-control" name="weight" placeholder="Enter Parcel Weight In kg..."/>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="form-group">
                    <label>Parcel Description </label>
                    <textarea class="form-control" name="desc" rows="3" placeholder="Enter ..."></textarea>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="form-group">
                    <label>note </label>
                    <textarea class="form-control" name="note" rows="3" placeholder="Enter ..."></textarea>
                  </div>
                </div>

    
                <div class="col-md-12">
                  <div class="form-group">
                    <button class="btn btn-primary" >Add new parcel</button>
                  </div>
                </div>



              </div> 
            </div><!-- /.box-body -->
          </form>
        </div><!-- /.box -->

        

      </div><!--left row -->
      <!-- <div class="col-md-4 bg-success">
        helo
      </div> -->
      
    </div><!-- /.row -->

  </section><!-- /.content -->

</div><!-- /.content-wrapper -->
<footer class="main-footer">
