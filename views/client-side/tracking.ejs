        <div class="row">
            <div class="col-md-12 tableContainer">
                <table id="example1" class="container table table-bordered table-striped">
                    <thead>
                      <tr>
                        <th>#</th>
                        <th>track_id</th>
                        <th>Current_Location</th>
                        <th>description</th>
                        <th>isuue </th>
                        <th>Resolve_note</th>
                        <th>Status</th>
                        <th>Executed_Time </th>
                        <th>Expected_arival </th>
                      </tr>
                    </thead>
                    <tbody>
                      <% if(reports.length > 0) { %>
                        <% let x = 1 %>
                        <% reports.forEach(parcel => { %>
                          <tr class="text-capitalize">
                              <td><%= x++ %></td>
                              <td><%= parcel.tracking_id %></td>
                            <% if(typeof parcel.current_location == "object"){ %>
                              <td>warehouse</td>
                            <% }else{ %>
                                <td><%= parcel.current_location %></td>
                            <% } %>
                            

                            <td > <%= parcel.description %> </td>
                            <td > <%= parcel.issue_type %> </td>
                            <td > <%= parcel.resolution %> </td>
                            <% if(parcel.status == "in transit"){ %>
                              <td class="text-primary text-center text-bold" style="text-decoration: none; color: blue;"><%= parcel.status %></td>
                            <% }else if(parcel.status == "delayed"){ %>
                              <td class="text-primary text-center text-bold" style="text-decoration: none; color: red;"><%= parcel.status %></td>
                            <% }else if(parcel.status == "resolved"){ %>
                              <td class="text-primary text-center text-bold"  style="text-decoration: none; color: black;"><%= parcel.status %></td>
                            <% }else if(parcel.status == "arrived" || parcel.status == "delivered"){ %>
                              <td class="text-primary text-center text-bold"  style="text-decoration: none; color: green;"><%= parcel.status %></td>
                            <% }else if(parcel.status == "arrived late"){ %>
                              <td class="text-primary text-center text-bold" style="text-decoration: none; color: orange;"><%= parcel.status %></td>
                            <% }else{ %>
                              <td class=" text-center text-bold"> <%= parcel.status %></td>

                            <% } %>
                            <td>
                              <%= parcel.created_at %>
                            </td>                            
                            <td>
                              <%= parcel.expected_arrival_time %>
                            </td>                            
                          </tr>
                        <% }); %>
                      <% }else{ %>
                        <tr>
                          <td colspan = "9">
                            No Record
                          </td>
                        </tr>
                      <% }; %>
                      
                    </tbody>
                    <tfoot>
                      <tr>
                        <th>#</th>
